server {
  domain: "google.com";
  scheme: "https";
  port: "443";

  location "/api/router" {
    match $path "^/api/(.*)$";
    parse $query_string;
    var $sb "hello_this_is_enginx_2_0";
    encode $sb;
    return "https://h5.ele.me$path$path/$#foo/$1$1$1/$0*$0*$0$sb";
  }
  location "/shop/?" {
    return "https://h5.ele.me";
  }
}

server {
